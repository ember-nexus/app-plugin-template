{
  "name": "@ember-nexus/app-plugin-template",
  "version": "0.1.9",
  "type": "module",
  "engines": {
    "node": ">= 20.0"
  },
  "browserslist": [
    "last 1 years and not dead"
  ],
  "keywords": [
    "ember-nexus-plugin"
  ],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && pnpm build:esm && pnpm build:browser",
    "build:browser": "vite build --config vite.config.ts",
    "build:esm": "pnpm tsc -b tsconfig.esm.json && pnpm cpy 'src/**/*' '!src/**/*.ts' dist/esm && vite build --config vite.esm.config.ts && node ./build-esm.mjs && rm ./dist/esm/style.css && cp ./dist/css/style.css ./dist/esm/global.css && rm -rf ./dist/css",
    "cs": "eslint ./src",
    "cs:fix": "eslint ./src --fix",
    "test:unit": "vitest run ./test/Unit",
    "test:unit:coverage": "vitest run ./test/Unit --coverage",
    "prepare": "ts-patch install",
    "storybook": "storybook dev -p 6006 -h 0.0.0.0 --no-open",
    "storybook:build": "storybook build"
  },
  "author": "Syndesi <info@ember-nexus.dev>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@awesome.me/webawesome": "^3.2.1",
    "@ember-nexus/app-core": "^0.1.4",
    "@lit/reactive-element": "^2.1.2",
    "daisyui": "^5.5.18",
    "lit": "^3.3.2",
    "lucide-static": "^0.564.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/web-components-vite": "^10.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.13",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.7",
    "@vitest/ui": "^4.0.18",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-storybook": "^10.2.8",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "ignore-loader": "^0.1.2",
    "msw": "^2.12.10",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "sinon": "^21.0.1",
    "storybook": "^10.2.8",
    "strong-mock": "^9.1.0",
    "tailwindcss": "^4.1.11",
    "terser": "^5.46.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tslib": "^2.8.1",
    "tsutils": "~3.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./global.css": {
      "default": "./dist/esm/global.css"
    },
    "./Component": {
      "import": {
        "types": "./dist/types/Component/index.d.ts",
        "default": "./dist/esm/Component/index.js"
      }
    },
    "./Component/Demo": {
      "import": {
        "types": "./dist/types/Component/Demo/index.d.ts",
        "default": "./dist/esm/Component/Demo/index.js"
      }
    },
    "./Page": {
      "import": {
        "types": "./dist/types/Page/index.d.ts",
        "default": "./dist/esm/Page/index.js"
      }
    }
  }
}
