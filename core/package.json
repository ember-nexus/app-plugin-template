{
  "name": "@ember-nexus/app-plugin-template",
  "version": "0.0.7",
  "type": "module",
  "engines": {
    "node": ">= 20.0"
  },
  "browserslist": [
    "last 1 years and not dead"
  ],
  "keywords": [
    "ember-nexus-plugin"
  ],
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && pnpm build:esm && pnpm build:browser",
    "build:browser": "vite build",
    "build:esm": "pnpm tsc -b tsconfig.esm.json && pnpm cpy 'src/**/*' '!src/**/*.ts' dist/esm",
    "cs": "eslint ./src",
    "cs:fix": "eslint ./src --fix",
    "test:unit": "vitest run ./test/Unit",
    "test:unit:coverage": "vitest run ./test/Unit --coverage",
    "prepare": "ts-patch install",
    "storybook": "storybook dev -p 6006 -h 0.0.0.0 --no-open",
    "storybook:build": "storybook build"
  },
  "author": "Syndesi <info@ember-nexus.dev>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@ember-nexus/app-core": "^0.0.10",
    "daisyui": "^5.0.43",
    "lit": "^3.1.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-themes": "^10.1.4",
    "@storybook/web-components-vite": "^10.1.4",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.10.1",
    "@types/sinon": "^21.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vitest/ui": "^4.0.15",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-storybook": "^10.1.4",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "ignore-loader": "^0.1.2",
    "msw": "^2.0.2",
    "prettier": "^3.0.3",
    "rimraf": "^6.1.2",
    "sinon": "^21.0",
    "storybook": "^10.1.4",
    "strong-mock": "^9.0.1",
    "terser": "^5.44.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.0.2",
    "tslib": "^2.8.1",
    "tsutils": "~3.21",
    "typescript": "^5.5",
    "vite": "^7.0.0",
    "vitest": "^4.0.15"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
